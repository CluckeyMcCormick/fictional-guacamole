// XSM Tutorial - Extended State Machine Tutorial
// Basic Extended State Machine, Flow - Graphviz Dot
// Directed Graph
digraph G {
    overlap=false; // Nodes are not allowed to overlap.
    compound=true; // Allow edges between clusters

    P_ON [label="Power On"];
    P_OFF [label="Power Off"];

    edge [label = ""];
    Root -> P_ON;
    Root -> P_OFF;
}
